<?xml version="1.0"?>
<launch>

<!-- Start the main plotting interface and console-->
<group>
  <node name="pressure_plot"
        pkg="rqt_multiplot"
        type="rqt_multiplot"
        args="--force-discover --multiplot-config $(find pressure_controller_ros)/config/plotting/4channels.xml --multiplot-run-all" />

  <node name ="console" pkg="rqt_console" type="rqt_console" />
</group>


</launch>